{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
	  "coverage/lcov-report/*.js",
	  "main.js",
	  "release-builds/"
		]
	},
	"rules": {
		"object-literal-sort-keys": false,
		"interface-name": false,
		"member-access": [true, "no-public"],
		"ordered-imports": false,
		"no-console": false,
		"jsx-no-lambda": false
	}
}
